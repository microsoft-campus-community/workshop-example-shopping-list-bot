{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ],
      "route": "GetItemsFunction/{conversationID}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "name": "getShoppingListDocument",
      "databaseName": "shopping-list-db",
      "collectionName": "ShoppingListContainer",
      "connectionStringSetting": "SHOPPING_LIST_COSMOSDB",
      "direction": "in",
      "sqlQuery": "SELECT c.item FROM c WHERE c.conversationID = {conversationID}"
  }
  ],
  "scriptFile": "../dist/GetItemsFunction/index.js"
}
